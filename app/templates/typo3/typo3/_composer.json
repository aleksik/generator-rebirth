{
  "name": "<%= appNameDasherize %>/typo3",
  "description" : "<%= appNameHumanize %> Typo3 Installer",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }, {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "<%= appNameSpace %>\\<%= appNamePascalize %>\\": "typo3conf/ext/<%= dir %>/Classes"
    }
  },
  "require": {
    "typo3/cms-base-distribution": "~7.6.0",
    "fluidtypo3/vhs": "*",
    "fluidtypo3/flux": "*",
    "fluidtypo3/fluidpages": "*",
    "fluidtypo3/fluidcontent": "*",
    "fluidtypo3/builder": "*",
    "helhum/realurl": "*",
    "helhum/typo3-console": "^4.0",
    "typo3-ter/news": "*",
    "typo3-ter/powermail": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "./"
    }
  }
}
